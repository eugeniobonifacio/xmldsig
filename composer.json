{
    "name": "eugeniobonifacio/xmldsig",
    "description": "Tool for easy management of XML Signatures (http://www.w3.org/TR/xmldsig-core/)",
    "license": "BSD-2-Clause",
    "keywords": [
        "xml",
        "xmldsig",
        "signature",
        "security",
        "soap"
    ],
    "authors": [
        {
            "name": "Maks3w",
            "email": "github.maks3w@virtualplanets.net"
        },
        {
            "name": "Eugenio Bonifacio",
            "email": "mail@eugeniobonifacio.com"
        }
    ],
    "homepage": "https://github.com/eugeniobonifacio/xmldsig/",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/eugeniobonifacio/xmlseclibs"
      }
    ],
    "require": {
        "eugeniobonifacio/xmlseclibs": "^3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.10.*",
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "FR3D\\XmlDSig\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FR3D\\XmlDSigTest\\": ["test"]
        }
    }
}
